{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": {
            "options": ["single", "avoid-template"]
        },
        "max-classes-per-file": false,
        "object-literal-sort-keys": false,
        "arrow-parens": {
            "options": "ban-single-arg-parens"
        },
        "member-ordering": false
    },
    "rulesDirectory": []
}